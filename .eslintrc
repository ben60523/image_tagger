{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "folder": ["./renderer"],
    "env": {
        "browser": true,
        "node": true,
        "jest": true,
        "commonjs": true
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-props-no-spreading": ["error", {"custom": "ignore"}],
        "react/prop-types": 0,
        "linebreak-style": 0
    }
}
  