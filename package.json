{
  "name": "media_tagger",
  "version": "1.8.2",
  "description": "",
  "main": "./build/main.js",
  "productName": "media_tagger",
  "scripts": {
    "start:test": "electron .",
    "start": "cross-env NODE_ENV=development webpack --config ./webpack.main.config.js && concurrently \"npm:watch\" \"electron .\"",
    "build:main": "cross-env NODE_ENV=production webpack --config ./webpack.main.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./webpack.renderer.config.js",
    "build": "npm run build:main && npm run build:renderer",
    "analyse": "webpack --config ./webpack.renderer.config.js --mode=production --env.analyse=true",
    "lint": "eslint ./renderer",
    "watch": "cross-env NODE_ENV=development webpack --config ./webpack.renderer.config.js --watch --watch-poll",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "clean": "rm -rf build dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://owenchang1992@github.com/owenchang1992/media_viewer.git"
  },
  "keywords": [],
  "author": "Chin Che Chang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/owenchang1992/media_viewer/issues"
  },
  "build": {
    "appId": "Media Tagger",
    "productName": "Media Tagger",
    "files": [
      "**/*",
      "public",
      "extra_res",
      "!node_module",
      "!renderer",
      "!src/main.js"
    ],
    "extraResources": [
      {
        "from": "./extra_res/bin",
        "to": "./extra_res/bin"
      }
    ],
    "mac": {
      "artifactName": "media_tagger_${version}_setup.${ext}",
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "artifactName": "media_tagger_${version}_setup.${ext}",
      "target": [
        {
          "arch": [
            "ia32",
            "x64"
          ],
          "target": "nsis"
        }
      ]
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "async-foreach": "^0.1.3",
    "crypto-js": "^4.0.0",
    "electron-localshortcut": "^3.2.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.6.0",
    "moment": "^2.29.1",
    "nedb": "^1.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "styled-components": "^5.1.1",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "electron": "^9.4.4",
    "electron-builder": "^22.8.0",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^27.0.6",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "browserslist": []
}
