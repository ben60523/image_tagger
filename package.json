{
  "name": "media_viewer",
  "version": "0.1.0",
  "description": "",
  "main": "./build/main.js",
  "scripts": {
    "start:test": "electron build/main.js",
    "start": "webpack --config ./webpack.main.config.js --mode=development && concurrently \"npm:watch\" \"electron ./build/main.js\"",
    "build:main": "webpack --config ./webpack.main.config.js --mode=production",
    "build:renderer": "webpack --config ./webpack.renderer.config.js --mode=production",
    "build": "npm run build:main && npm run build:renderer",
    "analyse": "webpack --config ./webpack.renderer.config.js --mode=production --env.analyse=true",
    "lint": "eslint ./renderer",
    "watch": "webpack --config ./webpack.renderer.config.js --mode=development --watch --watch-poll",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "clean": "rm -rf build dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://owenchang1992@github.com/owenchang1992/media_viewer.git"
  },
  "keywords": [],
  "author": "Chin Che Chang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/owenchang1992/media_viewer/issues"
  },
  "build": {
    "appId": "media-manager",
    "productName": "media-manager",
    "files": [
      "**/*",
      "assets",
      "!node_module",
      "!renderer",
      "!src/main.js"
    ],
    "mac": {
      "artifactName": "media-manager.${ext}",
      "category": "public.app-category.developer-tools"
    },
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.3.0",
    "css-loader": "^4.2.1",
    "electron": "^9.2.1",
    "electron-builder": "^22.8.0",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.0.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12"
  },
  "browserslist": []
}
